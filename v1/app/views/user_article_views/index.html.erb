<p id="notice"><%= notice %></p>

<h1>Listing User Article Views</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_article_views.each do |user_article_view| %>
      <tr>
        <td><%= link_to 'Show', user_article_view %></td>
        <td><%= link_to 'Edit', edit_user_article_view_path(user_article_view) %></td>
        <td><%= link_to 'Destroy', user_article_view, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

